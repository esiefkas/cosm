(ns cosm.rider
  (:require [ring.util.response :as ring]))


(defn handler [request]
  (println "HERE!!!!")
  (ring/response {:message "hello world"}))

(defn get-all-of-type [type]
  (let [db (d/db @datomic-conn)]
    (d/q '[:find (pull ?e [*])
           :in $ ?type
           :where
           [?e :cosm/type ?type]] db type)))